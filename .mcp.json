{
  "inputs": [
    {
      "id": "ado_org",
      "type": "promptString",
      "description": "Azure DevOps organization name  (e.g. \u0027contoso\u0027)"
    }
  ],
  "servers": {    
    "ado": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@azure-devops/mcp",
        "${input:ado_org}"
      ]
    },
    "StockTrading": {
      "type": "stdio",
      "command": "dotnet",
      "args": [
        "run",
        "--project",
        "AiStockTradeApp.McpServer/AiStockTradeApp.McpServer.csproj"
      ]
    }
  }
}